{
  "name": "jest-react-package-config-example",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.4",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.ts",
      "jest-canvas-mock"
    ]
  }
}
