{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vitest+React Example",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/build-dev",
        "${workspaceFolder}/examples/vitest-react"
      ],
      "outFiles": ["${workspaceFolder}/build-dev/**/*.js"],
      "sourceMaps": true
    },
    {
      "name": "Jest+React Example",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/build-dev",
        "${workspaceFolder}/examples/jest-react"
      ],
      "outFiles": ["${workspaceFolder}/build-dev/**/*.js"],
      "sourceMaps": true
    },
    {
      "name": "Jest+React with package.json config Example",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/build-dev",
        "${workspaceFolder}/examples/jest-react-with-packagejson-config"
      ],
      "outFiles": ["${workspaceFolder}/build-dev/**/*.js"],
      "sourceMaps": true
    }
  ]
}
