{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vitest+React Example",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/extension/dist",
        "${workspaceFolder}/examples/vitest-react"
      ],
      "outFiles": ["${workspaceFolder}/packages/extension/dist/**/*.js"],
      "sourceMaps": true
    },
    {
      "name": "Jest+React Example",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/extension/dist",
        "${workspaceFolder}/examples/jest-react"
      ],
      "outFiles": ["${workspaceFolder}/packages/extension/dist/**/*.js"],
      "sourceMaps": true
    }
  ]
}
